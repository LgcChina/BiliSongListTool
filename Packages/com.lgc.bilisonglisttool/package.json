  "author": {
    "name": "LgcChina",
    "url": "https://github.com/LgcChina"
  },
  "vpmDependencies": {
    "com.vrchat.worlds": "3.x.x"
  },
  "keywords": [ "VRChat", "video player", "Tool", "Unity" ],
  "samples": [
    {
      "displayName": "示例场景与预设",
      "description": "未包含示例场景与预设",
      "path": "Samples~/QuickStart"
    }
  ],
  "migrations": {
    "1.0.0": {
      "description": "删除Assets中手动导入的旧版B站歌单工具文件",
      "operations": [
        {
          "type": "delete",
          "paths": [
            "Assets/LGC/歌曲列表处理器/Editor"
          ]
        }
      ]
    }
  }
}
